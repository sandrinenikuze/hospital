<%= form_tag "/services/#{@service.id}", method: :patch do %>
  <div>
    Name: <input type="text" name="name" value="<%=@service.name%>">
  </div>
  <div>
      Description: <input type="text" name="description" value="<%=@service.description%>">
  </div>
  <div>
    <input type="submit" name="Update Hospital">
  </div>
<% end %>